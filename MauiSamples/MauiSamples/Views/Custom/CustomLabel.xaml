<?xml version="1.0" encoding="utf-8" ?>
<ContentView
  xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
  x:Class="MauiSamples.Views.Custom.CustomLabel"
  x:Name="CustomLabelView">

  <Label
    x:Name="TextLabel"
    BindingContext="{Binding Source={x:Reference CustomLabelView}}"
    Text="{Binding Text}"/>

</ContentView>
